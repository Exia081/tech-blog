{
  "buildCommand": "hugo version && hugo --gc --minify",
  "outputDirectory": "public",
  "installCommand": "git submodule update --init --recursive",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "HUGO_VERSION": "0.148.2"
    }
  }
}
